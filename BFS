#include<bits/stdc++.h>
using namespace std;

int vis[1000];
int cost[1000];
int par[1000];
vector<int>adj[1000];
queue<int>q;

void BFS(int source){
 vis[source]=1;
cost[source]=0;
 par[source]= source;
 q.push(source);

while(!q.empty()){
    int u = q.front();
    q.pop();

    int v;                                           //1: 2,3
    for(int i=0;i<adj[u].size();i++){
        v = adj[u][i];

        if(vis[v]==0){
            vis[v]=1;
            cost[v]= cost[u]+1;
            par[v]=u;

            q.push(v);
        }
    }



}

}
int main(){
    int node,edge;
    cin>>node>>edge;
    int u,v;
    for(int i=1;i<=edge;i++){
        cin>>u>>v;
        adj[u].push_back(v);
        adj[v].push_back(u);
    }
    int start,dest;
    cin>>start>>dest;

    BFS(start);

    cout<<cost[dest]<<endl;




}
