#include<bits/stdc++.h>
using namespace std;

void bubble(int arr[],int n){
    for(int i=0;i<n;i++){
        for(int j=i+1;j<n;j++){
            if(arr[j]<arr[i]){
               int temp = arr[j];
               arr[j]=arr[i];
               arr[i]= temp;
        }
    }

}
}

int main(){
           int arr[]={4,1,5,2,3};
    int n=5;
    bubble(arr,n);
    for(int i=0;i<n;i++){
        cout<<arr[i]<<" ";
    }




}
